<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h2>View Years for <%= department.name %></h2>

  <table border="1">
    <thead>
        <th>Year Name</th>
        <th>Manage Year</th>
 
    </thead>
    <tbody>
        <% years.forEach(function(year) { %>
            <tr>
                <td><%= year.year %></td>
                <td><a href="/years/<%=year.id%>/divisions/view">Manage</a></td>
            </tr>
            
        <% }); %>
    </tbody>
</table>
  
</body>
</html>


